{
  "name": "@evo-ninja/agent-utils",
  "version": "0.1.0",
  "license": "MIT",
  "main": "./build/index.js",
  "scripts": {
    "build": "rimraf build && tsc",
    "codegen": "yarn codegen:shims && yarn codegen:js-engine && yarn codegen:agent-plugin",
    "codegen:shims": "rollup -c",
    "codegen:js-engine": "polywrap codegen -m ./src/scripts/js-engine/polywrap.yaml --codegen-dir ./src/scripts/js-engine/types",
    "codegen:agent-plugin": "polywrap codegen -m ./src/scripts/agent-plugin/polywrap.yaml --codegen-dir ./src/scripts/agent-plugin/types",
    "test": "jest --runInBand --verbose --rootDir . --config ../../jest.config.js",
    "lint": "eslint --color -c ../../.eslintrc.js src/"
  },
  "dependencies": {
    "@polywrap/client-js": "~0.12.0",
    "@polywrap/plugin-js": "~0.12.0",
    "@polywrap/result": "~0.12.0",
    "axios": "^0.26.0",
    "chalk": "5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "figlet": "~1.6.0",
    "fuse.js": "^6.6.2",
    "fuzzysort": "^2.0.4",
    "json5": "^2.2.3",
    "openai": "~3.3.0",
    "path-browserify": "1.0.1",
    "string-similarity": "^4.0.4",
    "base64-to-uint8array": "1.0.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-inject": "^5.0.3",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "dotenv": "~16.3.1",
    "rollup": "^3.28.0",
    "tslib": "^2.6.2",
    "@types/figlet": "~1.5.6",
    "@types/jest": "29.5.0",
    "@types/path-browserify": "1.0.0",
    "@types/uuid": "9.0.6",
    "jest": "29.5.0",
    "polywrap": "~0.12.0",
    "rimraf": "~5.0.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
