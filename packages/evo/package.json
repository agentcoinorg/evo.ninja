{
  "name": "@evo-ninja/evo-agent",
  "version": "0.1.0",
  "license": "MIT",
  "main": "./build/index.js",
  "scripts": {
    "build": "rimraf build && tsc",
    "codegen": "yarn codegen:js-engine && yarn codegen:agent-plugin",
    "codegen:js-engine": "polywrap codegen -m ./src/wrap/js-engine/polywrap.yaml --codegen-dir ./src/wrap/js-engine/types",
    "codegen:agent-plugin": "polywrap codegen -m ./src/agent-plugin/polywrap.yaml --codegen-dir ./src/agent-plugin/types",
    "test": "jest --passWithNoTests --runInBand --verbose --rootDir . --config ../../jest.config.js"
  },
  "dependencies": {
    "@polywrap/client-js": "~0.12.0",
    "@polywrap/result": "~0.12.0",
    "@polywrap/sys-config-bundle-js": "~0.12.0",
    "@polywrap/web3-config-bundle-js": "~0.12.0",
    "@evo-ninja/agent-utils": "~0.1.0",
    "@evo-ninja/js-script-writer-agent": "~0.1.0",
    "axios": "^0.26.0",
    "fuse.js": "^6.6.2",
    "json5": "^2.2.3",
    "openai": "~3.3.0",
    "path-browserify": "1.0.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.0",
    "@types/path-browserify": "1.0.0",
    "jest": "29.5.0",
    "polywrap": "~0.11.0",
    "rimraf": "~5.0.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
